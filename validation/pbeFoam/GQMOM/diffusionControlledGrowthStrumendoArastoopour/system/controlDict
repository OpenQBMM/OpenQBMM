FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     pbeFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         20;

deltaT          10;

adjustTimeStep  off;

writeControl    runTime;

writeInterval   1;

purgeWrite      0;

writeFormat     binary;

writePrecision  100;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

suppressSolverInfo yes;

maxCo           0.5;

maxDeltaT       0.1;

