FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nut.air;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions          [0 2 -1 0 0 0 0];

internalField       uniform 0;

boundaryField
{
    walls
    {
        type               nutkWallFunction;
        value              $internalField;
    }
    top
    {
        type               cyclic;
    }
    bottom
    {
        type               cyclic;
    }
    frontAndBackPlanes
    {
        type    empty;
    }
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
