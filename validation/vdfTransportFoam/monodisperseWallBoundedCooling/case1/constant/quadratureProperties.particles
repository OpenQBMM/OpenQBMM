FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      quadratureProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

fieldMomentInversion basicVelocityFieldMomentInversion;


basicVelocityMomentInversion
{
    type CHyQMOMPlus;
    basicQuadrature
    {
        univariateMomentInversion   Gauss;
    }
}

momentAdvection
{
    velocityMomentAdvection   firstOrderKinetic;
    left
    {
        type    fixedTemperature;
        T       uniform 0.5;
    }
    right
    {
        type    fixedTemperature;
        T       uniform 1.0;
    }
}

