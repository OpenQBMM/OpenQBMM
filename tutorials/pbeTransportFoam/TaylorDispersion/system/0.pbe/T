FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 0;

boundaryField
{
    topWall      
    {
        type            zeroGradient;
    }

    bottomWall      
    {
        type            zeroGradient;
    }

    inlet      
    {
        type            zeroGradient;
    }

    outlet      
    {
	 		type            fixedValue;
         value           uniform 0;
    }

    frontAndBack    
    {
        type            empty;
    }
}
