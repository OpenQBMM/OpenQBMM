FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      moment.0;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    topWall      
    {
        type            zeroGradient;
    }

    bottomWall      
    {
        type            zeroGradient;
    }

    outlet      
    {
        type            zeroGradient;
    }

    inlet      
    {
        type            fixedValue;
         value           uniform 1;
    }

    frontAndBack    
    {
        type            empty;
    }
}


